<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.netforceinfotech.admindashboard">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Speaker.AddSpeaker" />
        <activity android:name=".Speaker.EditSpeaker" />
        <activity android:name=".Categories.AddCategories" />
        <activity android:name=".Categories.EditCategories" />
        <activity
            android:name=".Categories.Category"
            android:label="@string/title_activity_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Speaker.Speaker" />
        <activity android:name=".Content.Content" />
        <activity android:name=".Content.AddContent" />
        <activity android:name=".Content.EditContent" />
        <activity android:name=".Login.LoginActivity"></activity>
    </application>

</manifest>